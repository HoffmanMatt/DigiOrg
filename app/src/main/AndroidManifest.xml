        <activity
            android:name="com.DigiOrg.EditReminderDateTime"
            android:label="@string/title_activity_edit_reminder_date_time" >
        </activity>
        <activity
            android:name="com.DigiOrg.EditReminderNormal"
            android:label="@string/title_activity_edit_reminder" >
        </activity>
        <activity
            android:name="com.DigiOrg.EditReminderByDays"
            android:label="@string/title_activity_edit_reminder_by_days" >
        </activity>
        <activity
            android:name="com.DigiOrg.ReminderDualEdit"
            android:label="@string/title_activity_reminder_dual_edit" >
        </activity>
    </application>

</manifest>
